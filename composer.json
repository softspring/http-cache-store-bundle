{
    "name": "softspring/http-cache-store-bundle",
    "description": "This is a template for creating new components",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.51.*",
        "phpstan/phpstan": "^1.10",
        "psr/cache": "^3.0",
        "symfony/cache": "^5.4|^6.0|^7.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Bundle\\HttpCacheStoreBundle\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
